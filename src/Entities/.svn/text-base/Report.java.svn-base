package Entities;

public class Report {
	private String jbid;
	private int numJbOp;
	private int numCV;
	private int numOffer;
	private int numRefuse;
	private int numOnboard;
	private String rid;
	
	public String getJbid(){
		return jbid;
	}
	
	public void setJbid(String jbid){
		this.jbid = jbid;
	}
	
	public int getNumJbOp(){
		return numJbOp;
	}
	
	public void setNumJbOp(int numJbOp){
		this.numJbOp = numJbOp;
	}
	
	public int getNumCV(){
		return numCV;
	}
	
	public void setNumCV(int numCV){
		this.numCV = numCV;
	}
	
	public int getNumOffer(){
		return numOffer;
	}
	
	public void setNumOffer(int numOffer){
		this.numOffer = numOffer;
	}
	
	public int getNumRefuse(){
		return numRefuse;
	}
	
	public void setNumRefuse(int numRefuse){
		this.numRefuse = numRefuse;
	}
	
	public int getNumOnboard(){
		return numOnboard;
	}
	
	public void setNumOnboard(int numOnboard){
		this.numOnboard = numOnboard;
	}

	public String getRid(){
		return rid;
	}
	
	public void setRid(String rid){
		this.rid = rid;
	}
	
	public void addNumOffer(int numOffer){
		this.numOffer += numOffer;
	}
	
	public void addNumRefuse(int numRefuse){
		this.numRefuse += numRefuse;
	}
	
	public void addNumOnboard(int numOnboard){
		this.numOnboard += numOnboard;
	}

}
